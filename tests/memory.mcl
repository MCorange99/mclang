include "std.mcl"

// write "abc" into the memory
mem 0 + 97 store
mem 1 + 98 store
mem 2 + 99 store
mem 3 + 10 store

// print "abc" to stdout
4 mem puts

// increament each character by 1 making it "bcd"
mem 0 + dup load 1 + store
mem 1 + dup load 1 + store
mem 2 + dup load 1 + store

// print "bcd" to stdout
4 mem puts

// print UINT64_MAX (Largest 64 bit word)
mem 18446744073709551615 store64 
mem load64 print