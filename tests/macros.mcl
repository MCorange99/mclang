include "std.mcl"

macro check_less
  < if
    "YES\n" puts
  else
    "NO\n" puts
  end
end

1 2 check_less
2 1 check_less

macro even_fibs
  0 1 while over 1000000 < do
    over 2 mod 0 = if
      over print
    end
    swap over +
  end
  drop drop
end

"------------------------------\n" puts

even_fibs
